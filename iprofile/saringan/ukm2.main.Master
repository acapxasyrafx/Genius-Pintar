                        <asp:Label ID="lblTimeTaken" runat="server" Text="0" Visible="true"></asp:Label>&nbsp;
                        <asp:Label ID="lblDebug" runat="server" Text="" Visible="true"></asp:Label>&nbsp;
                        <asp:Label ID="lblExamStopTimeHide" runat="server" Text="" Visible="true"></asp:Label>&nbsp;|&nbsp;
